<RaiseFault name='RF-MissingAuthzHeader'>
    <DisplayName>RF-MissingAuthzHeader</DisplayName>
    <Description></Description>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
    <FaultResponse>
        <Set>
    <Payload contentType="application/json">{
  "error" :  {
    "code" : 10016,
    "message" : "No authorization request supplied."
  }
}
</Payload>
            <StatusCode>400</StatusCode>
            <ReasonPhrase>Bad Request</ReasonPhrase>
        </Set>
    </FaultResponse>
</RaiseFault>
